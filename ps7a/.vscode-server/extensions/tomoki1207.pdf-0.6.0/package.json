{
	"name": "pdf",
	"displayName": "vscode-pdf",
	"description": "Display pdf file in VSCode.",
	"version": "0.6.0",
	"publisher": "tomoki1207",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"pdf",
		"viewer"
	],
	"icon": "vscode-pdf.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/tomoki1207/vscode-pdfviewer.git"
	},
	"bugs": {
		"url": "https://github.com/tomoki1207/vscode-pdfviewer/issues"
	},
	"activationEvents": [
		"onLanguage:pdf",
		"onCommand:extension.pdf-preview",
		"onWebviewPanel:pdf.preview"
	],
	"main": "./out/src/extension",
	"contributes": {
		"languages": [
			{
				"id": "pdf",
				"aliases": [
					"Plain Text (PDF)",
					"pdf"
				],
				"extensions": [
					".pdf"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "pdf",
				"scopeName": "text.plain, text.plain.pdf",
				"path": "./syntaxes/pdf.tmLanguage"
			}
		],
		"commands": [
			{
				"command": "extension.pdf-preview",
				"title": "View PDF"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceLangId == pdf",
					"command": "extension.pdf-preview",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"package": "vsce package"
	},
	"dependencies": {
		"@types/vscode": "^1.44.0"
	},
	"devDependencies": {
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.12.21",
		"mocha": "^6.2.2",
		"tslint": "^5.20.1",
		"typescript": "^3.7.3",
		"typings": "^1.4.0",
		"vsce": "^1.71.0",
		"vscode-test": "^1.3.0"
	},
	"__metadata": {
		"id": "4386e6f6-ec10-4463-9d23-c24278718947",
		"publisherId": "36dae390-559e-4374-bca8-a49ec1616c36",
		"publisherDisplayName": "tomoki1207"
	}
}